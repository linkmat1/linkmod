{{ form_start(form, {attr: {class: 'stacked'}}) }}
<div class="grid">
    {{ form_row(form.title) }}
    {{ form_row(form.slug) }}
    {{ form_row(form.isOnline, {row_attr: {style: 'align-self: flex-end'}}) }}

</div>
<div class="full space02">
    {{ form_row(form.shortdesc) }}
</div>
