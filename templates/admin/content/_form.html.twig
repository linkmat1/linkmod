{{ form_start(form, {attr: {class: 'stacked'}}) }}
<div class="grid">
{{ form_row(form.title) }}
    {{ form_row(form.slug) }}
    {{ form_row(form.category) }}
    {{ form_row(form.created_at) }}
    {{ form_row(form.publish_at) }}
    {{ form_row(form.author) }}
    {{ form_row(form.reference) }}
        <div class="grid-small">
        {{ form_row(form.isOk) }}
        {{ form_row(form.isOnline) }}

    </div>
    <div class="grid-small">
    {{ form_row(form.upnews) }}

    </div>
</div>
<div class="full">


    {{ form_row(form.content) }}
</div>

<div class="full">
    <button class="btn btn-big">{{ button_label|default('Save') }}</button>
    <div class="btn-big btn"><a href="{{ path('content_index') }}">Annul√©</a></div>
</div>


{{ form_end(form) }}
